manner(?e, ?q) :- isa(?e, Old) and subject(?e, ?s) and born(?s, ?b) and time(?e, ?t) and diff_years(?b, ?t, ?q)
born(?s, ?t) :- name(?s, ?n) and BIRTHDATE(?n, ?t)
die(?s, ?t) :- name(?s, ?n) and DEATHDATE(?n, ?t)
time(?e, ?t) :- at_time(?e, ?f) and time1(?f, ?t)
time1(?e, ?t) :- isa(?e, Die) and subject(?e, ?s) and die(?s, ?t)